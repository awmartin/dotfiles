#         case "$PATH" in
#             *:"$dir":*|*:"$dir"|"$dir":*|"$dir") ;;
#             *) PATH="$PATH:$dir" ;;
#         esac
#     fi
# done

export PATH

# Set overrides for packages in the ~/local directory.
source "$HOME/dotfiles/scripts/set_local_package_paths"
