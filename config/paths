bin_home="$HOME/bin"
bin_usr_local="/usr/local/bin"

path_high_priority=( $bin_home $bin_usr_local )

# High priority PATH
for dir in $path_high_priority; do
    if [ -d "$dir" ]; then
        #case "$PATH" in
        #    *"$dir"*) ;;
        #    *) PATH="$dir:$PATH" ;;
        #esac
        PATH="$dir:$PATH"
    fi
done

